  options :
   -
     name : OPT_NOUVEAU
     identifier : 0x2374f7c6586a23a3
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1+
          
          
          // Si on n'a pas enregistré le kanban en cours
          SI FichierKanban.Valeur = "" ALORS
          	
          	// Si le kanban en cours a été modifié
          	SI KANBAN_TACHES.Modifié = Vrai ALORS
          		
          		SI pas ( LISTE_ENATTENTE.Carte.Vide _ET_ LISTE_ENCOURS.Carte.Vide _ET_ LISTE_TERMINE.Carte.Vide ) ALORS
          			
          			// Demande confirmation pour l'enregistrement
          			SELON Confirmer( Annuler, "Les modifications n'ont pas été enregistrées. Souhaitez-vous enregistrer le kanban maintenant ?" )
          			
          				// Enregistre
          				CAS Oui
          					
          					// Si l'enregistrement échoue, on annule
          					OPT_SAUVER.ExécuteTraitement( trtOptionMenu )
          					
          				// Annule
          				CAS Annuler
          					DonneFocusEtRetourUtilisateur()
          					
          				// Poursuit
          				CAS Non
          			FIN
          		FIN
          		
          	FIN
          FIN
          
          // Supprime toutes les cartes
          KANBAN_TACHES.SupprimeToutCarte()
          
          // RAZ
          KANBAN_TACHES.Modifié = Faux
          FichierKanban.RAZ()
         type : 32
   -
     name : OPT_OUVRIR
     identifier : 0x2374f7cd586b3f3a
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1+
          
          URISélecteur( procédure( bSuccès est un booléen, Fichier est une URI){
          	
          	SI bSuccès ALORS
          		
          		// Démarre sur un kanban vierge
          		OPT_NOUVEAU.ExécuteTraitement( trtOptionMenu )
          		
          		// Mémorise
          		FichierKanban = Fichier
          				
          		// Charge et désérialise
          		URIVersBuffer( FichierKanban, procédure( bRésultat est un booléen, bufKanban est un buffer, sFormat est une chaîne) {
          						
          			quand exception dans
          						
          				jsDescriptionKanban	est un json = bufKanban
          				
          				si jsDescriptionKanban["ListeEnAttente"]..Existe ALORS
          					// Ajoute les cartes dans le kanban
          					pour tout jsCarte de jsDescriptionKanban["ListeEnAttente"]
          						LISTE_ENATTENTE.AjouteCarte( JSONVerskbCarte( jsCarte ) )
          					FIN
          				FIN
          				
          				si jsDescriptionKanban["ListeEnCours"]..Existe ALORS
          					// Ajoute les cartes dans le kanban
          					pour tout jsCarte de jsDescriptionKanban["ListeEnCours"]
          						LISTE_ENCOURS.AjouteCarte( JSONVerskbCarte( jsCarte ) )
          					FIN
          				FIN
          				
          				si jsDescriptionKanban["ListeTerminée"]..Existe ALORS
          					// Ajoute les cartes dans le kanban
          					pour tout jsCarte de jsDescriptionKanban["ListeTerminée"]
          						LISTE_TERMINE.AjouteCarte( JSONVerskbCarte( jsCarte ) )
          					FIN
          				FIN
          				
          			faire
          				erreur( "Le kanban n'a pas pu être chargé avec les données mémorisées" )
          				retour
          			FIN
          			
          		} )
          		
          	FIN
          	
          } )
         type : 32
   -
     name : OPT_SAUVER
     identifier : 0x2374f807586c219b
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1+
          
          // Si le kanban est vide
          SI LISTE_ENATTENTE.Carte.Vide _ET_ LISTE_ENCOURS.Carte.Vide _ET_ LISTE_TERMINE.Carte.Vide ALORS
          	
          	// Si on est sur un nouveau kanban
          	SI FichierKanban.Valeur = "" ALORS
          		DonneFocusEtRetourUtilisateur( KANBAN_TACHES )
          	FIN	
          FIN
          
          // Si on n'a pas de fichier
          SI FichierKanban.Valeur = "" ALORS	
          	URISélecteur( Sauvegarde, "", uricreation ) 	
          FIN
          
          Sauvegarde( Vrai, FichierKanban )
          
          	PROCÉDURE INTERNE Sauvegarde( bSuccès est un booléen, Fichier est une uri )
          		
          		// Si la sélection a réussi
          		SI bSuccès ALORS
          			
          			// Mémorise 
          			FichierKanban = Fichier
          			
          			jsDescriptionKanban est un json
          						
          			pour toute Carte de LISTE_ENATTENTE.Carte
          				ajoute( jsDescriptionKanban.ListeEnAttente, kbCarteVersJSON( Carte ) ) 
          			FIN
          			
          			pour toute Carte de LISTE_ENCOURS.Carte
          				ajoute( jsDescriptionKanban.ListeEnCours, kbCarteVersJSON( Carte ) ) 
          			FIN
          			
          			pour toute Carte de LISTE_TERMINE.Carte
          				ajoute( jsDescriptionKanban.ListeTerminée, kbCarteVersJSON( Carte ) ) 
          			FIN
          			
          			// Sérialise et sauve sur le disque
          			BufferVersURI( jsDescriptionKanban..FormatJSON, FichierKanban, uriCréation )
          			
          		FIN
          	FIN
          
         type : 32
