 code_elements :
  type_code : 1
  p_codes :
   -
     code : |1-
      PROCEDURE MaFenêtre()
      
      DescriptionKanban est une structure
      	ListeEnAttente est un tableau de kbCartes
      	ListeEnCours   est un tableau de kbCartes
      	ListeTerminée  est un tableau de kbCartes
      fin
      
      FichierKanban est un uri
   -
     code : |1-
      
      si TypeEcran() = teTablette ALORS
      	KANBAN_TACHES.ModeAffichage = modeAffichageKanbanMultiListe
      FIN
     type : 34
   -
     code : |1+
      
      SI MaFenêtreFille.Nom = FEN_KBCARTE_MOBILE..Nom ALORS
      	
      	si MaFenêtreFille.ValeurRenvoyée = vrai alors
      		
      		si FEN_KBCARTE_MOBILE.ModeAjout alors
      			
      			// Exécute le traitement d'ajout de carte
      			SI KANBAN_TACHES.ExécuteTraitement( trtAjoutCarteKanban, FEN_KBCARTE_MOBILE.gCarte, FEN_KBCARTE_MOBILE.ListeCarte ) = Faux ALORS
      				RETOUR
      			FIN
      			
      			// Ajoute la carte dans la liste
      			nIndice est un entier = FEN_KBCARTE_MOBILE.ListeCarte.AjouteCarte( FEN_KBCARTE_MOBILE.gCarte )
      			SI nIndice > 0 ALORS
      				
      				// On se positionne sur la carte ajoutée		
      				FEN_KBCARTE_MOBILE.ListeCarte.PositionListe( FEN_KBCARTE_MOBILE.ListeCarte.Carte[ nIndice ] )
      			FIN
      			
      		sinon
      		
      			KANBAN_TACHES.Modifié = Vrai
      		fin
      	fin
      FIN
     type : 177
   -
     type : 180
     enabled : false
   -
     type : 230
